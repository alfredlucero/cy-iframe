{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["src/**/*.ts", "types/**/*.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "cypress",
    "samplecypressconsumerjs",
    "samplecypressconsumerts"
  ]
}
